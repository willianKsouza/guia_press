<%- include('../../partials/header') %>
<%- include('../../partials/navbar') %>

<body>
  <div class="container">
    <hr>
    <h2>Usuarios</h2>
    <a href="/admin/users/create">Criar novo usuario</a>
    <hr>
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>
                    Id
                </th>
                <th>
                    Email
                </th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user => { %>
                <tr>

                    <td> <%= user.id %> </td>
                    <td> <%= user.email %> </td>
                </tr>


            <%}) %>
        </tbody>
    </table>




  </div>  

</body>



<%- include('../../partials/footer') %>